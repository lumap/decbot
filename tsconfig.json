{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "lib": [
            "ES2020",
            "ES2021"
        ],
        "outDir": "./dist/",
        "module": "commonjs",
        "target": "ES2020",
        "strict": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": false
    },
    "include": [
        "./src"
    ],
    "exclude": [
        "./mode_modules"
    ]
}